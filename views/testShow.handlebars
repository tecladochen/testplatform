 <div class="content" style="padding-top: 20px;">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">{{test.title}}</h3>

                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse" title="折叠">
                          <i class="fas fa-minus"></i>
                        </button>
                        </div>
                    </div>
                    <div class="card-body">
                        {{#each test.content}}
                        <div class="row">
                            <div class="form-group col-12" style="margin-bottom: 25px;">
                                <div class="input-group mb-12">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">题目</span>
                                    </div>
                                    <input type="text" class="form-control" value="{{this.question}}" readonly>
                                </div>
                                <div class="row" style="margin-top: 10px;">
                                    <div class="input-group mb-6 col-6">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">选项A</span>
                                        </div>
                                        <input type="text" class="form-control" value="{{this.selectA}}" readonly>
                                    </div>
                                    <div class="input-group mb-6 col-6">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">选项B</span>
                                        </div>
                                        <input type="text" class="form-control" value="{{this.selectB}}" readonly>
                                    </div>
                                </div>
                                <div class="row" style="margin-top: 10px;">
                                    <div class="input-group mb-6 col-6">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">选项C</span>
                                        </div>
                                        <input type="text" class="form-control" value="{{this.selectC}}" readonly>
                                    </div>
                                    <div class="input-group mb-6 col-6">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">选项D</span>
                                        </div>
                                        <input type="text" class="form-control" value="{{this.selectD}}" readonly>
                                    </div>
                                </div>
                                <div class="input-group mb-6 col-2" style="margin-top: 10px;">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">参考答案</span>
                                    </div>
                                    <input type="text" class="form-control" value="{{this.answer}}" readonly>
                                </div>
                                <div class="input-group mb-6 col-12" style="margin-top: 10px;">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">解析</span>
                                    </div>
                                    <input type="text" class="form-control" value="{{this.analyze}}" readonly>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
                <div class="row ">
                    <div class="col-12 ">
                        <a href="http://localhost:3000/testManage" class="btn btn-secondary ">返回</a>
                    </div>
                </div>
            </div>